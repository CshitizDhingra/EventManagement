        </td>
        <td>
            @Html.DisplayFor(modelItem => item.edate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gathering)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.d_price)
        </td>
        <td>
