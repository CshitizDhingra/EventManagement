        <th>
            @Html.DisplayNameFor(model => model.edate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gathering)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.d_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.f_price)
